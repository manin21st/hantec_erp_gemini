on w_web_browser.create
this.sle_1=create sle_1
this.ole_1=create ole_1
this.Control[]={this.sle_1,&
this.ole_1}
end on

on w_web_browser.destroy
destroy(this.sle_1)
destroy(this.ole_1)
end on

event open;Integer  li_idx

li_idx = w_mdi_frame.dw_listbar.InsertRow(0)
w_mdi_frame.dw_listbar.SetItem(li_idx,'window_id',Upper(This.ClassName()))
w_mdi_frame.dw_listbar.SetItem(li_idx,'window_name','[WWW]')
w_mdi_frame.Postevent("ue_barrefresh")

is_window_id = this.ClassName()

w_mdi_frame.st_window.Text = Upper(is_window_id)
end event

event resize;//ole_1.object.width = this.width
//ole_1.object.height = this.height

/******************************************************
 
  파워빌더(PBU) 길이와 OLEControl(웹 브라우저)의 길이 
 
  width  : 3000    =>    645
  height : 2000    =>    470

*****************************************************/
ole_1.object.Width( (645 * This.Width) / 3000 )
ole_1.object.Height( (470 * This.Height) / 2000  )
end event

event closequery;w_mdi_frame.st_window.Text = ''

long li_index

li_index = w_mdi_frame.dw_listbar.Find("window_id = '" + Upper(This.ClassName()) + "'", 1, w_mdi_frame.dw_listbar.RowCount())

w_mdi_frame.dw_listbar.DeleteRow(li_index)
w_mdi_frame.Postevent("ue_barrefresh")
end event

