event dragdrop;DragObject			ldo_control
vuo_emp_info		luo_emp_info
vuo_emp_info		luo_new
Integer				li_newx, li_newy
datawindow			ldw_emp
long					ll_row

ldo_control = DraggedObject()
choose case TypeOf(ldo_control)
	case userobject!
		luo_emp_info = ldo_control
		li_newx = This.PointerX() - luo_emp_info.ii_clicked_x
		li_newy = This.PointerY() - luo_emp_info.ii_clicked_y
		If li_newx < 1 Then
			li_newx = 1
		Else
			If Mod(li_newx, 100) > 50 Then
				li_newx += (100 - Mod(li_newx, 100))
			Else
				li_newx -= Mod(li_newx, 100)
			End If
		End If
		If li_newy < 1 Then
			li_newy = 1
		Else
			If Mod(li_newy, 50) > 25 Then
				li_newy += (50 - Mod(li_newy, 50))
			Else
				li_newy -= Mod(li_newy, 50)
			End If
		End If

		luo_emp_info.Move(li_newx, li_newy)
		This.SetRedraw(False)
		luo_emp_info.uf_redraw_lines(1)	// calltype : 1 -> default, 2 -> prevent recursion
		This.SetRedraw(True)
		ib_chabged = True

	Case datawindow!
		ldw_emp = source

		if ldw_emp.dataobject <> 'd_org_dept_member' then return

		ll_row = ldw_emp.GetRow()
		//wf_emp_info_release()
		
		OpenUserObject(luo_new, PointerX(), PointerY())
		luo_new.iw_parent  = this
		luo_new.BringToTop = true
		luo_new.DragAuto   = true
		
		ii_emp_info_ub ++
		luo_new.ii_index = ii_emp_info_ub
		emp_info[luo_new.ii_index] = luo_new
		
		luo_new.idw_emp_list = source
		luo_new.is_emp_cd = Trim(ldw_emp.object.empno[ll_row])	//사번
		luo_new.uf_set_emp_info(luo_new.is_emp_cd)
		wf_emp_info_release()
//		wf_set_changed(true)
		ib_chabged = true
end choose


end event

event open;Integer		li_index
Long			ll_count

im_pop = Create m_org_emp_info_popup

For ll_count = 1 To UpperBound(This.Control[])
	If TypeOf(This.Control[ll_count]) = Line! Then
		If IsNumber(Mid(ClassName(This.Control[ll_count]), 4, 10)) Then
		li_index ++
		lines_info[li_index].line_object = This.Control[ll_count]
		End If
	End If
Next

dw_dept_emp.SetTransObject(SQLCA)
end event

event activate;If IsValid(w_org_pmis_emp_info) Then
	If Not w_org_pmis_emp_info.Visible Then
		w_org_pmis_emp_info.Event Post ue_visible()
	End If
End If
end event

event deactivate;
If IsValid(w_org_pmis_emp_info) Then
	w_org_pmis_emp_info.Event Post ue_visible()
End If

end event

event close;Integer	li_index

// AutoSave!

Destroy m_org_emp_info_popup

If IsValid(w_org_pmis_emp_info) Then
	Close(w_org_pmis_emp_info)
End If

For li_index = 1 To ii_emp_info_ub
	CloseUserObject(emp_info[li_index])
Next
end event

event rbuttondown;If ib_moveatom Then
	wf_emp_info_release()
End If
end event

