public function integer wf_emp_info_delete (vuo_emp_info au_emp_info);Long	ll_count, ll_index

If isnull(UpperBound(au_emp_info.emp_info_child)) Then
	ll_count = 0
Else
	ll_count = UpperBound(au_emp_info.emp_info_child)
End If

For ll_index = 1 To ll_count
	If IsValid(au_emp_info.emp_info_child[ll_index]) Then
		// child exist
		MessageBox('삭제 실패', "하위 구성이 존재합니다.", Stopsign!)
		Return -1
	End If
Next

ll_count = UpperBound(au_emp_info.depend_line_index)
For ll_index = 1 To ll_count
	If au_emp_info.depend_line_index[ll_index] > 0 Then
		lines_info[au_emp_info.depend_line_index[ll_index]].line_object.Visible = False
		lines_info[au_emp_info.depend_line_index[ll_index]].used = False
	End If
Next

If au_emp_info.ii_index = ii_emp_info_ub Then
	ii_emp_info_ub --
End If
CloseUserObject(au_emp_info)
ib_chabged = True
Return 1
end function

