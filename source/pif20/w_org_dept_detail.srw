event ue_newatom();vuo_emp_info	luo_new

wf_emp_info_release()

OpenUserObject(luo_new, 150, 150)
luo_new.iw_parent  = This
luo_new.BringToTop = True
luo_new.DragAuto   = True

ii_emp_info_ub ++
luo_new.ii_index = ii_emp_info_ub
emp_info[luo_new.ii_index] = luo_new

ib_chabged = True

end event

event ue_connect1();wf_emp_info_freeze()

connect_mode = "parent"

SetNull(emp_info_parent)
SetNull(emp_info_child)

Pointer = ".\bmp\cross_p.cur"
//Pointer = "c:\mis\image\general\cross_p.cur"
end event

event ue_connect2();wf_emp_info_freeze()

connect_mode = "parent2"

SetNull(emp_info_parent)
SetNull(emp_info_child)

Pointer = ".\bmp\cross_p.cur"
//Pointer = "c:\mis\image\general\cross_p.cur"
end event

event ue_addemp;Window	lw_search

//OpenWithParm(lw_search, This, "w_dept_diagram_detail_emp")
OpenWithParm(lw_search, idw_emplist, "w_org_dept_detail_emp")
//lw_search.Dynamic wf_set_targetdw(idw_emplist)
end event

event ue_openorg();vuo_emp_info		luo_new[]
Integer				li_index, li_readcount

wf_reset_org()

Send(Handle(This), 277, 6, 0)	// Scroll To Top
Send(Handle(This), 276, 5, 0)	// Scroll To Left edge

dw_dept_emp.Retrieve(is_org_cd)
li_readcount = dw_dept_emp.RowCount()
For li_index = 1 To li_readcount
	Trigger OpenUserObject(luo_new[li_index], dw_dept_emp.object.POS_X[li_index], dw_dept_emp.object.POS_Y[li_index])

	ii_emp_info_ub ++
	luo_new[li_index].DragAuto = ib_moveatom
	luo_new[li_index].is_emp_cd = dw_dept_emp.object.emp_cd[li_index]
	luo_new[li_index].iw_parent = This
	luo_new[li_index].ii_index = li_index
	luo_new[li_index].connect_mode = Trim(dw_dept_emp.object.connect_mode[li_index])
	
	luo_new[li_index].depend_line_index[1] = dw_dept_emp.object.line_index_1[li_index]
	luo_new[li_index].depend_line_index[2] = dw_dept_emp.object.line_index_2[li_index]
	luo_new[li_index].depend_line_index[3] = dw_dept_emp.object.line_index_3[li_index]

	luo_new[li_index].uf_set_emp_info(dw_dept_emp.object.emp_cd[li_index])

	emp_info[luo_new[li_index].ii_index] = luo_new[li_index]
Next

For li_index = 1 To li_readcount
	If dw_dept_emp.object.parent[li_index] > 0 Then
		emp_info_parent = luo_new[dw_dept_emp.object.parent[li_index]]
		Choose Case luo_new[li_index].connect_mode
			Case 'parent'
				wf_emp_info_set_child(luo_new[li_index])
			Case 'parent2'
				wf_emp_info_set_child2(luo_new[li_index])
			Case Else
		End Choose
	End If
Next

If Not ib_moveatom Then
